    "scripts": {
        "start": "npm run clean && cd ../../ && cross-env NODE_ENV=development babel-node $NODE_DEBUG_OPTION src/server/index.js",
        "start:prod": "npm run clean && npm run build:main && npm run serve",
        "serve": "cd ../../ && cross-env NODE_ENV=production babel-node build/ssr/index.js",
        "build:main": "concurrently \"npm run build:client\" \"npm run build:server\" && npm run build:node",
        "build:client": "cd ../../ && rm -rf build/client && cross-env NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --progress --config webpack/ssr/client.js && cd -",
        "build:server": "cd ../../ && rm -rf build/server && cross-env NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --progress --config webpack/ssr/server.js && cd -",
        "build:node": "cd ../../ && cross-env NODE_ENV=production babel src/server/index.js -o build/ssr/index.js && ./node_modules/.bin/terser build/ssr/index.js -o build/ssr/index.js && cd -",
        "clean": "cd ../../ && rm -rf build && cd -",
        "storybook": "cd ../../ && start-storybook -p 6006 && cd -",
        "build-storybook": "cd ../../ && build-storybook && cd -"
    }
